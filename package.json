{
  "name": "gokv",
  "version": "0.0.12",
  "description": "A global, low-latency, and druable key-value store.",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./types/core.d.ts",
  "sideEffects": false,
  "exports": {
    "./web": {
      "module": "./dist/web.mjs",
      "types": "./types/web.d.ts"
    },
    "./web-polyfill": {
      "module": "./dist/web-polyfill.mjs"
    }
  },
  "files": [
    "dist/",
    "types/",
    "web-polyfill.mjs"
  ],
  "scripts": {
    "build": "pnpm esbuild --bundle --minify --platform=browser --format=esm --target=es2022 --sourcemap --outfile=dist/index.mjs mod.ts",
    "build:web": "pnpm esbuild --bundle --minify --platform=browser --format=esm --target=es2022 --sourcemap --outfile=dist/web.mjs web.ts",
    "prepublishOnly": "pnpm build && pnpm build:web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ije/gokv.git"
  },
  "bugs": {
    "url": "https://github.com/ije/gokv/issues"
  },
  "homepage": "https://gokv.io",
  "author": "ije",
  "license": "MIT",
  "dependencies": {
    "@peculiar/webcrypto": "^1.4.0",
    "node-fetch": "^3.2.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "esbuild": "^0.15.9"
  }
}
